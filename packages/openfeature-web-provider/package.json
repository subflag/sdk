{
  "name": "@subflag/openfeature-web-provider",
  "version": "0.2.0",
  "description": "OpenFeature web provider for Subflag feature flags",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting not yet configured'",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openfeature",
    "feature-flags",
    "subflag",
    "web",
    "browser"
  ],
  "author": "Subflag",
  "license": "MIT",
  "peerDependencies": {
    "@openfeature/web-sdk": "^1.0.0"
  },
  "devDependencies": {
    "@openfeature/web-sdk": "^1.6.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
